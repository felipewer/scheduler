{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": [
      "es6",
      "es7",
      "es2017",
      "dom"
    ]
  },
  "include": [
    "src/**/*",
    "/build/"
  ],"exclude": [  
    "/dist/",
    "**/*.spec.ts"
  ]
}