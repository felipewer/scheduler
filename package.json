{
  "name": "scheduler",
  "version": "1.0.0",
  "description": "A time scheduler",
  "main": "index.js",
  "scripts": {
    "start": "parcel -p 8080 index.html",
    "build": "parcel build src/widget.ts -o scheduler-widget --no-source-maps",
    "test": "jest --watch"
  },
  "author": "Felipe Carlos Werlang",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "@types/parcel-env": "0.0.0",
    "@types/react-dom": "^16.0.7",
    "jest": "^23.1.0",
    "parcel-bundler": "^1.8.1",
    "ts-jest": "^22.4.6"
  },
  "dependencies": {
    "formik": "^1.0.2",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "openzeppelin-solidity": "1.11.0",
    "preact": "^8.2.9",
    "preact-compat": "^3.18.0",
    "preact-habitat": "3.2.1",
    "react-datepicker": "^1.5.0",
    "typescript": "^3.0.1",
    "web3": "1.0.0-beta.35",
    "yup": "^0.26.2"
  },
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
